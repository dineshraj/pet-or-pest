{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "moduleResolution": "node",
    "sourceMap": true,
    "jsx": "preserve",
    "resolveJsonModule": true,
    "noEmit": true,
    "esModuleInterop": true,
    "allowJs": true,
    "checkJs": false,
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true
  }
}
